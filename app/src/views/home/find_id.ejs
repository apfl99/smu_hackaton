<body data-spy="scroll" data-target=".onpage-navigation" data-offset="60">
   <div class="container">
    <div class="container-login">
      <form action="/find_id" class ="form-login">
        <div class="welcome">Find your account ID</div>
        <p style="color : rgba(190, 190, 190, 1);">Name</p>
        <div class="input-group mb-3">
          <input type="text" name="name" class="form-control" placeholder="Name">
          <span class="input-group-text"><i class="fa-solid fa-id-card"></i></span>
        </div>
        <p style="color : rgba(190, 190, 190, 1);">Email</p>
        <div class="input-group mb-4">
          <input type="email" name="email" class="form-control" placeholder="E-mail">
          <span class="input-group-text"><i class="fa-solid fa-at"></i></span>
        </div>
        <div class="account-options">
          <a href="./register">Create account</a>
          <a href="find-pw">Forgot password</a>
        </div>
        <button type="submit">Find ID</button>
      </form>
    </div>
  </div>

  
